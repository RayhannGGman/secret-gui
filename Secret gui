local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("SECRET GUI", "Ocean")

-- MAIN
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")


MainSection:NewButton("Kohls admin house Scripts(Credits to Owner) ", "Use this on Kohls admin house", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/FunzysDeath/Corexious/main/Corexious%20V1"))()
end)


MainSection:NewButton("Pop it trading Scripts only(Credits to Owner) ", "use it!!", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/Kiroftt/popit/main/scripx'))();
end)


MainSection:NewButton("The Second of kohls admin house script(Credits to Owner)", "More good than first", function()
    loadstring(game:HttpGet('https:/raw.githubusercontent.com/quivings/cmdy/main/main'))()
end)

MainSection:NewButton("Auto Speed(FOR SPEED RUN SIMULATOR ONLY) ", "NOT BAD!!", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/iMBY1tAJ"))()
end)


MainSection:NewButton("Remote Spy(Credits to Owner) ", "Universal!!", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/78n/SimpleSpy/main/SimpleSpySource.lua"))()
end)


local MainSection = Main:NewSection("My Scripts")

MainSection:NewButton("Infinite Cash Elephant Hotel(I Made) ", "GOOD", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/qQFkykFY"))()
end)

local MainSection = Main:NewSection("Toggle Scripts")

MainSection:NewToggle("AutoFarm(Strong Blade Simulator", "Good for PRO!!", function(state)
    if state then
        while wait() do
game:GetService("ReplicatedStorage"):WaitForChild("Common"):WaitForChild("Library"):WaitForChild("Network"):WaitForChild("RemoteFunction"):InvokeServer("S_System_Sword_Slash",{})
end
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    end
end)

MainSection:NewToggle("AutoFarm (Lucky Block Battlegrounds) ", "GOOD!!", function(state)
    if state then
        while wait() do
game:GetService("ReplicatedStorage"):WaitForChild("SpawnLuckyBlock"):FireServer()
end
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    end
end)

MainSection:NewToggle("AutoRebirth(Strong Blade Simulator) ", "good", function(state)
    if state then
        while wait() do
game:GetService("ReplicatedStorage"):WaitForChild("Common"):WaitForChild("Library"):WaitForChild("Network"):WaitForChild("RemoteFunction"):InvokeServer("DOREBIRTH",{})
end
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    end
end)

